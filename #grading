g=int(input("enter your marks:"))
if g==100:
    print("s")
elif g>90 and g<100:
    print("A")
elif g>80 and g<90:
    print("B")
elif g>70 and g<80:
    print("C")
elif g>60 and g<70:
    print("D")
elif g>50 and g<60:
    print("E")
elif g<50:
    print("F")
else:
    print("invalid input")
    
